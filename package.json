{
  "name": "@spudnova-ui/components",
  "author": "Kelly Keating",
  "license": "ISC",
  "keywords": [
    "ui",
    "react",
    "css",
    "components"
  ],
  "description": "UI module with styled React components",
  "version": "0.2.1",
  "type": "module",
  "main": "dist/spudnova-ui.umd.js",
  "module": "dist/spudnova-ui.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "vite build && npx tsc --project tsconfig.build.json",
    "publish": "npm publish --access=public",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,scss,md}'"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.4",
    "jsdom": "^26.0.0",
    "prettier": "^3.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/components.es.js",
      "require": "./dist/components.umd.js"
    },
    "./style.css": "./dist/components.css"
  }
}
